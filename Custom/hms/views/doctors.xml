<odoo>
    <data>
        <record id="hms_doctor" model="ir.actions.act_window">
            <field name="name">Doctor</field>
            <field name="res_model">hms.doctor</field>
        </record>

        <record id="hms_doctor_view" model="ir.ui.view">
            <field name="name">hms Doctor</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="first_name"/>
                    <field name="image"/>
                </tree>
            </field>
        </record>

        <menuitem id="hms_doctor_menu" name="Doctor" parent="hms_root_menu" action="hms_doctor" />
    </data>
</odoo>